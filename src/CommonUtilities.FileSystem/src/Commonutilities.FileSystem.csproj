<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFrameworks>netstandard2.0;netstandard2.1;net48</TargetFrameworks>
    <Nullable>enable</Nullable>
    <LangVersion>latest</LangVersion>

    <RootNamespace>Sklavenwalker.CommonUtilities.FileSystem</RootNamespace>
    <AssemblyName>Sklavenwalker.CommonUtilities.FileSystem</AssemblyName>
    <SignAssembly>True</SignAssembly>
    <Authors>Anakin Sklavenwalker</Authors>
    <Description>Helper classes and methods targeting the Filesystem and Paths.</Description>
    <Copyright>Anakin Sklavenwalker</Copyright>
    <RepositoryUrl>https://github.com/AnakinSklavenwalker/CommonUtilities</RepositoryUrl>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <EnableNETAnalyzers>True</EnableNETAnalyzers>
    <AnalysisLevel>latest</AnalysisLevel>
    <GenerateDocumentationFile>True</GenerateDocumentationFile>
    <AssemblyOriginatorKeyFile>..\..\..\AnakinOSS.snk</AssemblyOriginatorKeyFile>
  </PropertyGroup>

</Project>
