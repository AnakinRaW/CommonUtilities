<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFrameworks>netstandard2.0;netstandard2.1;net48;net5</TargetFrameworks>
    <Nullable>enable</Nullable>
    <LangVersion>latest</LangVersion>
    <RootNamespace>Sklavenwalker.CommonUtilities</RootNamespace>
    <AssemblyName>Sklavenwalker.CommonUtilities</AssemblyName>
    <SignAssembly>True</SignAssembly>
    <Authors>Anakin Sklavenwalker</Authors>
    <Description>Shared set of common classes and utilities for all of my software.</Description>
    <Copyright>Anakin Sklavenwalker</Copyright>
    <RepositoryUrl>https://github.com/AnakinSklavenwalker/CommonUtilities</RepositoryUrl>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <EnableNETAnalyzers>True</EnableNETAnalyzers>
    <AnalysisLevel>latest</AnalysisLevel>
    <GenerateDocumentationFile>True</GenerateDocumentationFile>
    <AssemblyOriginatorKeyFile>..\..\..\AnakinOSS.snk</AssemblyOriginatorKeyFile>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="System.IO.Abstractions" Version="13.2.47" />
    <PackageReference Include="Validation" Version="2.5.42" />
  </ItemGroup>

</Project>
